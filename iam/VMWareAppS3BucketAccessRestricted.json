{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VMWareS3BucketAccessPolicy",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObjectVersion",
                "s3:GetBucketTagging",
                "s3:GetObject",
                "s3:GetObjectTagging",
                "s3:GetObjectVersion",
                "s3:ListBucket",
                "s3:PutObject",
                "s3:RestoreObject"
            ],
            "Resource": [
                "arn:aws:s3:::rabbitbird-training-bucket"
            ],
            "Condition": {
                "StringLike": {
                    "s3:prefix": [
                        "vmware/*"
                    ]
                }
            }
        }
    ]
}
